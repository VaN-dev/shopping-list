{% extends ':default:layout.html.twig' %}

{% block content %}
    <div class="container-fluid">
        <h1>Tous les tags</h1>

        <ul>
            {% for tag in tags %}
                <li><a href="{{ path('app.tag.read', {'name': tag.name}) }}">{{ tag.name }}</a></li>
            {% endfor %}
        </ul>
    </div>
{% endblock content %}